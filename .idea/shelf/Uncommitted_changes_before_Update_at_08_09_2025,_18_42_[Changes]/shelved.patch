+import { createFileRoute } from "@tanstack/react-router";
+import {
+	ContentExample,
+	ContentFooter,
+	ContentHeader,
+} from "../../../components";
+import { patterns } from "../../patterns/patterns.list.ts";
+import { alertInfo } from "./alert.info.tsx";
+
+export const Route = createFileRoute("/components/alert/")({
+	component: RouteComponent,
+});
+
+function RouteComponent() {
+	const { name, description, examples } = alertInfo;
+
+	const patternsIncludingComponent = patterns.filter(
+		(pattern) =>
+			pattern.implementedUsingComponents &&
+			pattern.implementedUsingComponents.includes(name),
+	);
+
+	return (
+		<main>
+			<ContentHeader name={name}>
+				<p>{description}</p>
+			</ContentHeader>
+			<ContentExample examples={examples}></ContentExample>
+			<ContentFooter
+				links={patternsIncludingComponent.map((pattern) => ({
+					href: `/patterns/${pattern.name}`,
+					title: `Mønster: ${pattern.name}`,
+				}))}
+			/>
+		</main>
+	);
+}
